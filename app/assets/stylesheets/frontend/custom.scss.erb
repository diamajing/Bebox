.notice-contact{
    position: fixed;
    width: 89%;
    margin: auto;
    z-index: 1000000000000;
    top: 50px;
    left: 6%;

}

.transparent{
    background-color: rgba(14, 14, 14, 0.68);
    border-color: #f7f7f7;
    color: #fdfdfd;
}

.btn-primary{
    background-color: #fdaf46;
    border-color: #FFC107;
}

.btn-primary:hover, .btn-primary:focus, .btn-primary:active, .btn-primary.active, .open > .btn-primary.dropdown-toggle {
    color: #fff;
    background-color: #f1a845;
    border-color: #091c39;
}

/*Parallax section*/

.parallax {
    /* The image used */
    background-image: url("<%= image_path 'rsz_onlineshop.jpg' %>");

    /* Set a specific height */
    min-height: 500px; 

    /* Create the parallax scrolling effect */
    background-attachment: fixed;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;

    h2{
        padding-top: 200px;
        color: white;
        font-family: 'Oswald', sans-serif;
        font-weight:100;
    }

    .orange{
        background-color: transparent;
        color: #FFC107;
        font-size: 24px;
    }
}